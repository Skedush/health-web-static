{
  "private": true,
  "scripts": {
    "build": "umi build",
    "build:test": "cross-env UMI_ENV=test umi build",
    "build:demo": "cross-env UMI_ENV=demo umi build",
    "build:prod": "cross-env UMI_ENV=prod umi build",
    "build:xinjiang": "cross-env UMI_ENV=xinjiang umi build",
    "lint": "eslint --ext '.js,.jsx,.ts,.tsx' src",
    "tsc": "tsc -p ./ --noEmit",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:nomock": "cross-env MOCK=none umi dev",
    "test": "umi test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "ignore": [
      "node_modules/**"
    ],
    "linters": {
      "*.{ts,tsx}": [
        "eslint --fix",
        "prettier --write",
        "git add"
      ],
      "*.{js,jsx}": [
        "eslint --fix",
        "prettier --write",
        "git add"
      ],
      "*.less": [
        "stylelint --syntax less",
        "prettier --write",
        "git add"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/core-decorators": "^0.20.0",
    "@types/dom-to-image": "^2.6.0",
    "antd": "^3.20.6",
    "axios": "^0.18.0",
    "classnames": "2.2.6",
    "dom-to-image": "^2.6.0",
    "dva": "^2.6.0-beta.6",
    "dva-model-creator": "^0.3.0",
    "dva-model-extend": "^0.1.2",
    "file-saver": "^2.0.2",
    "id-validator": "^1.3.0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^3.0.0",
    "platform": "^1.3.6",
    "prop-types": "^15.7.0",
    "qs": "^6.6.0",
    "react": "^16.12.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-image": "2.1.1",
    "react-images": "^1.0.0",
    "react-perfect-scrollbar": "^1.5.3",
    "slash2": "^2.0.0",
    "store": "^2.0.12",
    "url-parse": "^1.4.7",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "@angular/compiler": "^8.2.2",
    "@angular/core": "^8.2.2",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/classnames": "^2.2.9",
    "@types/express": "^4.17.1",
    "@types/jest": "^23.3.12",
    "@types/lodash": "^4.14.138",
    "@types/mockjs": "^1.0.2",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "antd-pro-merge-less": "^1.0.0",
    "antd-theme-webpack-plugin": "^1.3.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "codelyzer": "^5.1.0",
    "commitizen": "^3.1.1",
    "cross-env": "^5.2.0",
